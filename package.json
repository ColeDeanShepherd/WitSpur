{
  "name": "witspur.com",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "clean": "node_modules/.bin/ts-node build/clean.ts",
    "build:css": "node_modules/.bin/ts-node build/build-css.ts",
    "build:html": "node_modules/.bin/ts-node build/build-html.ts",
    "build:js": "webpack",
    "build": "npm run clean && npm run build:html && npm run build:css && npm run build:js",
    "build:watch": "node_modules/.bin/watch \"npm run build\" src",

    "test": "echo \"Error: no test specified\" && exit 1",

    "reload": "node_modules/.bin/browser-sync reload",
    "buildReload:watch": "node_modules/.bin/watch \"npm run build && npm run reload\" src",
    "browser:watch": "node_modules/.bin/browser-sync start --config bs-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ColeDeanShepherd@bitbucket.org/ColeDeanShepherd/witspur.com.git"
  },
  "author": "Cole Shepherd",
  "homepage": "https://bitbucket.org/ColeDeanShepherd/witspur.com#readme",
  "private": true,
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.3",
    "browser-sync": "^2.18.8",
    "concurrently": "^3.4.0",
    "fs-extra": "^3.0.1",
    "source-map-loader": "^0.2.1",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.2",
    "watch": "^1.0.2",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
